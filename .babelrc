{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [
          ".ios.ts",
          ".android.ts",
          ".ts",
          ".ios.tsx",
          ".android.tsx",
          ".jsx",
          ".js",
          ".json"
        ],
        "alias": {
          "@assets": "./assets",
          "@components": "./src/components",
          "@context": "./src/context",
          "@context/*": "./src/context/*",
          "@helper": "./src/helper",
          "@hooks": "./src/hooks",
          "@models": "./src/models",
          "@pages": "./src/pages",
          "@styles": "./src/styles",
          "@themes": "./src/themes",
          "@navigation": "./src/navigation"
        }
      }
    ]
  ]
}