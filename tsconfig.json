// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@assets": ["./assets"],
      "@components": ["./src/components"],
      "@context": ["./src/context"],
      "@context/*": ["./src/context/*"],
      "@helper": ["./src/helper"],
      "@hooks": ["./src/hooks"],
      "@models": ["./src/models"],
      "@pages": ["./src/pages"],
      "@styles": ["./src/styles"],
      "@themes": ["./src/themes"],
      "@navigation": ["./src/navigation"]
    }
  },
  "exclude": ["node_modules", "dist"]
}

